{
  "name": "codeanalyst-backend",
  "version": "1.0.0",
  "description": "Backend service for CodeAnalyst - AI-powered website analysis platform",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "start:local": "node start-without-redis.js",
    "start:ngrok": "node start-with-ngrok.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "worker": "node src/workers/analysisWorker.js",
    "build": "tsc",
    "lint": "eslint src/**/*.js",
    "docker:build": "docker build -t c-backend .",
    "docker:run": "docker run -p 3001:3001 codeanalyst-backend",
    "audit:fix": "npm audit fix --force",
    "update:check": "npm outdated",
    "build:vercel": "cd .. && npm run build:vercel"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.1",
    "@google-ai/generativelanguage": "^2.5.0",
    "axe-core": "^4.8.3",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cheerio": "^1.1.2",
    "chrome-launcher": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lighthouse": "^12.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "pa11y": "^8.0.0",
    "pg": "^8.11.3",
    "playwright": "^1.40.1",
    "puppeteer": "^23.10.4",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "website-analysis",
    "code-analysis",
    "seo",
    "accessibility",
    "security",
    "performance",
    "backend",
    "api"
  ],
  "author": "Eomcorner OU",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  }
}
